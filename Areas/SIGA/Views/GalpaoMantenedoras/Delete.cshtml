@model SIGA.Entities.Models.GalpaoMantenedora

@{
    ViewBag.Title = "Galpão x Mantenedora";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Delete</h2>

<h3>Tem certeza que deseja excluir este registro?</h3>
<div>
    <h4>Galpão x Mantenedora</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Galpao.NomeGalpao)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Galpao.NomeGalpao)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Mantenedora.NomeMantenedora)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Mantenedora.NomeMantenedora)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Excluir" class="btn btn-default" /> |
            @Html.ActionLink("Voltar", "Index")
        </div>
    }
</div>
